{
    "description": "IDN Homograph Attack Block for Firefox",
	"manifest_version": 2,
	"name": "FxIDNHomographAttackBlocker",
	"version": "1.2",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "browser_action": {
        "default_title": "Fx IDN Homograph Attack Blocker",
        "default_popup": "popup.html",
        "browser_style": true
      },
    "background": {
        "scripts": ["background.js", "punycode.js"],
        "persistent": false
      }
}